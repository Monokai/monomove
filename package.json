{
  "name": "@monokai/monomove",
  "version": "1.8.5",
  "author": "Monokai",
  "license": "MIT",
  "description": "utilities for moving things on screen",
  "repository": {
    "type": "git",
    "url": "https://github.com/monokai/monomove.git"
  },
  "type": "module",
  "sideEffects": false,
  "main": "./dist/umd/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./source": "./src/index.js"
  },
  "files": [
    "./dist",
    "./src"
  ],
  "browserslist": [
    ">1%, last 2 versions and > 0.05%, not dead"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:production": "rollup -c --environment BUILD:production",
    "watch": "rollup src/index.js --format umd --name monomove --watch --output.file=dist/umd/index.js"
  },
  "keywords": [
    "move",
    "tween",
    "scroll",
    "smooth"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@clarifynl/eslint-config-clarify": "^0.0.7",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "rollup": "^3.15.0",
    "rollup-plugin-delete": "^2.0.0"
  },
  "eslintConfig": {
    "extends": [
      "@clarifynl/clarify"
    ],
    "rules": {
      "import/extensions": 0
    }
  }
}
